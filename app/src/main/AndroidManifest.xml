<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jun.sampo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sampo_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sampo_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SAMPO_TITLE">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SAMPO_MAIN"></activity>
        <activity android:name=".SAMPO_HIGHSCORE"></activity>
    </application>

</manifest>